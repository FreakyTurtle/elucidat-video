{
  "name": "ElucidatVideo",
  "version": "0.4.1",
  "private": true,
  "author": "Freaky Turtle",
  "main": "public/electron.js",
  "productName": "Elucidat Video",
  "description": "Elucidat Video",
  "homepage": "./",
  "dependencies": {
    "electron-updater": "^2.23.3",
    "hark": "^1.2.0",
    "is-electron": "^2.1.0",
    "material-ui": "^0.20.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "socket.io-client": "^2.1.1",
    "webrtc-adapter": "^6.3.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000/ && electron .",
    "react-start": "BROWSER=none react-scripts start",
    "start": "ELECTRON_START_URL=http://localhost:3000/ concurrently \"npm run react-start\" \"wait-on http://localhost:3000 && npm run electron .\"",
    "pack": "build --dir",
    "dist": "build",
    "electron-pack": "build -mwl -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build",
    "gh-publish": "build -c.extraMetadata.main=build/electron.js -p always"
  },
  "build": {
    "appId": "com.elucidat.video",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "electron": "^2.0.4",
    "electron-builder": "^20.20.4",
    "electron-packager": "^12.1.0",
    "wait-on": "^2.1.0"
  }
}
